{
  "title": "Real-time tracking of dengue virus evolution",
  "maintainers": [
    {"name": "Ammar Aziz", "url": "https://github.com/ammaraziz/jev"}
  ],
  "build_url": "https://github.com/ammaraziz/jev",  
  "colorings": [
    {
      "key": "gt",
      "title": "Genotype",
      "type": "categorical"
    },
    {
      "key": "num_date",
      "title": "Date",
      "type": "continuous"
    },
    {
      "key": "Country",
      "title": "Country",
      "type": "categorical"
    },
    {
      "key": "region",
      "title": "Region",
      "type": "categorical"
    },
    {
      "key": "Host",
      "title": "Host",
      "type": "categorical"
    }
  ],
  "geo_resolutions": [
    "Country",
    "region"
  ],
  "display_defaults": {
    "map_triplicate": true,
    "color_by": "Host",
    "distance_measure": "div"
  },
  "filters": [
    "Country",
    "region"
  ]
